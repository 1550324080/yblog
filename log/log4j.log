 12			Stream<String> stream = list.stream();
 13			// 2、对Stream流中的元素分别进行过滤、截取操作
 14		       Stream<String> stream2 = stream.filter(i -> i.startsWith("张"));
 15			Stream<String> stream3 = stream2.limit(2);
